# A brown hut
Group {
	children [
	# Draw the hut walls
		Shape {
			appearance DEF Brown Appearance {
				material Material {
					diffuseColor 0.6 0.4 0.0
				}
			}
			geometry Cylinder {
				height 2.0
				radius 2.0
			}
		},
	# Draw the hut roof
		Transform {
			translation 0.0 2.0 0.0
			children Shape {
				appearance USE Brown
				geometry Cone {
					height 2.0
					bottomRadius 2.5
				}
			}
		}
	]
}
